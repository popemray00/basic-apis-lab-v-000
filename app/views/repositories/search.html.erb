<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>

  <% if @items %>

  <% @items.each do |item| %>
    <li>
      <%= item["full_name"] %>
      <%= item["description"] %>
      <%= item["html_url"] %>
    </li>

    <% end %>
    <% end %>
